      "source": [
        "import cmath\n",
        "\n",
        "# Solve the quadratic equation ax^2 + bx + c = 0\n",
        "\n",
        "a = 1\n",
        "b = 5\n",
        "c = 6\n",
        "\n",
        "# calculate the discriminant\n",
        "delta = (b**2) - 4*(a*c)\n",
        "\n",
        "# find two solutions\n",
        "x1 = (-b - cmath.sqrt(delta)) / (2 * a)\n",
        "x2 = (-b + cmath.sqrt(delta)) / (2 * a)\n",
        "\n",
        "print(f'The solutions are {x1} and {x2}')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
